{
    "isDev": false,
    "baseUrl": "%(nginx.url)s",
    "session": {
        "host": "localhost",
        "port": 6380,
        "db": 0,
        "secret": "bang!",
        "secure": true
    },
    "http": {
        "listen": "127.0.0.1",
        "port": %(nginx.proxy_port)s
    },
    "andyetAuth": {
        "id": "%(andyet.id)s",
        "secret": "%(andyet.secret)s"
    },
    "andyetAPIs": {
        "apps": "%(andyet.appsUrl)s",
        "shippy": "%(andyet.shippyAPI)s",
        "talky": "%(andyet.talkyAPI)s"
    },
    "bucker": {
        "console": true,
        "level": "debug",
        "logstash": {
            "redis": true,
            "host": "%(dns.logstash.ip)s",
            "key":  "bucker",
            "channel": true,
            "source_host": "%(hostname)s"
        }
    }
}