
backend api_andbang
    balance roundrobin
    server  andbang_api %(haproxy.api)s weight 1 maxconn 2500 check

backend acct_andbang
    balance roundrobin
    server  andbang_acct %(haproxy.acct)s weight 1 maxconn 2500 check

backend ws_andbang
    balance roundrobin
    server  andbang_api %(haproxy.ws)s weight 1 maxconn 2500 check
